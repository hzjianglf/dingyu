html>body .leftcolumn
{
  display: block;
}
* html .leftcolumn .inside
{
  display: block;
}
.leftcolumn
{
  float: left;
  overflow: visible;
  position: relative;
  z-index: 10;
  height: 100%;
}
.leftcolumn
{
  margin: 0 1px 0 -225px;
  width: 225px;
}

.outercolumncontainer
{
