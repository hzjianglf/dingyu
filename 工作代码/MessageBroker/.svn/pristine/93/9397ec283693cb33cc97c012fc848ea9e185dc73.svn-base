	public String createExecutionGroup() {
		title = "创建执行组";
		viewdescription = "在该页面用户可以查看创建执行组的状态。";
		String flag = null;
		
		BrokerInfo brokerInfo = new BrokerInfo();
		brokerInfo = (BrokerInfo)dao.findByQmgrname(qmgrname).get(0);
		
		BrokerProxy b = connect(brokerInfo.getHostname(), new Integer(brokerInfo.getPort()).intValue(),brokerInfo.getQmgrname());
		if (b != null) {
			ExecutionGroupProxy egProxy = null;
			
			try {
				b.setSynchronous(MAX_WAIT_TIME * 1000);			
				egProxy = b.createExecutionGroup(egname);
				
				if (egProxy != null) {
					setQmgrname(qmgrname);
					setAppname(appname);
					comment1 = "代理 " + brokerInfo.getBname() + " 执行组 " + egname + " 创建成功。";
					log.info(comment1);	
					flag = SUCCESS;
				} else {
					comment1 = "代理 " + brokerInfo.getBname() + " 执行组 " + egname + " 创建失败。";
					log.info(comment1);	
					flag = ERROR;				
				}
								
			} catch (ConfigManagerProxyLoggedException e) {				
				//e.printStackTrace();
				comment1 = "代理 " + brokerInfo.getBname() + " 执行组 " + egname + " 创建失败。";
				log.error(comment1);
				flag = ERROR;
			} catch (IllegalArgumentException e) {				
				//e.printStackTrace();
				comment1 = "代理 " + brokerInfo.getBname() + " 上创建执行组名称不能为空。";
				log.error(comment1);
				flag = ERROR;
			}
			
		} else {
			comment1 = "代理 " + brokerInfo.getBname() + " 连接失败。";
			log.error(comment1);
			flag = ERROR;
		}
		
		if (b != null) {
			b.disconnect();
		}
		
		return flag;
	}

