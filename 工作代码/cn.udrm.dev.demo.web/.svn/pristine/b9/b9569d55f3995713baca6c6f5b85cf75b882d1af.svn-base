/* body样式 */

html, body{
	font-size: 12pt;
	color: #000000;
	overflow: hidden;
	border: 0 none;
	height: 100%;
	font-family: arial, 宋体
}

A:link{text-decoration:none}
A:visited{text-decoration:none}
A:hover {color: #15428b;text-decoration:none}

table {
	font-size: 12pt;
	color: #000000;
	font-family: arial, 宋体
}

