.x-form-input-border {
	background: #dfe8f6;
	border-width: 0px;
}

.x-menutoolbar{border-color:#a9bfd3;border-style:solid;border-width:0 0 0 0;display:block;background:#d0def0 url(../images/head/menu_background.gif) repeat-x top left;position:relative;zoom:1;}

.x-selectable, .x-selectable * {   
    -moz-user-select: text!important;   
    -khtml-user-select: text!important;   
}

.x-fieldset legend {
    font:bold 12px tahoma, arial, helvetica, sans-serif;
    color:#15428b;
}

.complete .x-tree-node-anchor span {
    text-decoration: line-through;
    color: #777;
}

.upload-icon {
    background: url('/framework/images/image_add.png') no-repeat 0 0 !important;
}

