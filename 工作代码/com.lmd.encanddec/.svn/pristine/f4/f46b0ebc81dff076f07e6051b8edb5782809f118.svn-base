package com.lmd.encanddec.model;

import javax.persistence.Basic;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity(name = "JjmService")
@Table(name = "Jjm_Service")
public class JjmService {

	@Id()
	private String jjmId;
	
	@Basic(optional = true)
	private String jjmName;
	
	@Basic(optional = true)
	private String jjmInfo;
	
	@Basic(optional = true)
	private String jjmUseTime;

	public String getJjmId() {
		return jjmId;
	}

	public void setJjmId(String jjmId) {
		this.jjmId = jjmId;
	}

	public String getJjmName() {
		return jjmName;
	}

	public void setJjmName(String jjmName) {
		this.jjmName = jjmName;
	}

	public String getJjmInfo() {
		return jjmInfo;
	}

	public void setJjmInfo(String jjmInfo) {
		this.jjmInfo = jjmInfo;
	}

	public String getJjmUseTime() {
		return jjmUseTime;
	}

	public void setJjmUseTime(String jjmUseTime) {
		this.jjmUseTime = jjmUseTime;
	}
	
	
}
