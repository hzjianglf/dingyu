<?xml version="1.0" encoding="UTF-8"?>
<ServiceModule id="resCountManager" name="资源统计服务" version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ServiceModule.xsd">
   <ServiceComponent id="ResCountManager" name="资源统计服务" version="1.0">
      <service interface="lmd.res.count.service.ResCountManager" />
      <!-- 没有配置factory时查找顺序：1、从Spring容器中找serivce接口的唯一实现，2、从osgi的service注册表去找，3、new一个
        如果配置factory，则直接调用。最好是都托管给Spring容器。 -->
      <implementation class="lmd.res.count.service.impl.ResCountManagerImpl"/>
      <properties>
         <property name="firstKey" value="aStringValue"/>
         <property name="secondKey" value="anotherStringValue"/>
      </properties>
   </ServiceComponent>
   
   <ServiceComponent id="queryManager" name="sql查询服务" version="1.0">
      <service interface="lmd.res.count.service.QueryManager" />
      <!-- 没有配置factory时查找顺序：1、从Spring容器中找serivce接口的唯一实现，2、从osgi的service注册表去找，3、new一个
        如果配置factory，则直接调用。最好是都托管给Spring容器。 -->
      <implementation class="lmd.res.count.service.impl.QueryManagerImpl"/>
      <properties>
         <property name="firstKey" value="aStringValue"/>
         <property name="secondKey" value="anotherStringValue"/>
      </properties>
   </ServiceComponent>
</ServiceModule>
